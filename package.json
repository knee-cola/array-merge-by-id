    "babel-loader": "^8.0.0-beta.0",
    "clean-webpack-plugin": "^0.1.17",
    "jest": "^21.2.1",
    "jsdoc-to-markdown": "^3.0.2",
    "ts-jest": "^21.2.4",
    "ts-loader": "^3.1.1",
